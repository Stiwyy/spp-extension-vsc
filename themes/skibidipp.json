{
	"$schema": "vscode://schemas/color-theme",
	"name": "Skibidi++ Tokyo Night",
	"type": "dark",
	"colors": {
		"editor.background": "#1a1b26",
		"editor.foreground": "#a9b1d6",
		"editorLineNumber.foreground": "#565f89",
		"editorLineNumber.activeForeground": "#7aa2f7",
		"editor.selectionBackground": "#28344e",
		"editor.findMatchBackground": "#3d59a1",
		"editor.findMatchHighlightBackground": "#3d59a159",
		"editorCursor.foreground": "#c0caf5",
		"editor.lineHighlightBackground": "#292e42",
		"editorIndentGuide.background": "#292e42",
		"editorIndentGuide.activeBackground": "#3b4261"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#565f89"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#bb9af7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Storage",
			"scope": "storage.type",
			"settings": {
				"foreground": "#bb9af7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#9ece6a"
			}
		},
		{
			"name": "Numbers and Booleans",
			"scope": ["constant.numeric", "constant.language"],
			"settings": {
				"foreground": "#ff9e64"
			}
		},
		{
			"name": "Functions",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#7aa2f7"
			}
		},
		{
			"name": "Variables",
			"scope": "variable.other",
			"settings": {
				"foreground": "#c0caf5"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#89ddff"
			}
		},
		{
			"name": "CONSOLE - CYAN",
			"scope": "skibidipp.console",
			"settings": {
				"foreground": "#2ac3de",
				"fontStyle": "bold"
			}
		},
		{
			"name": "BUILT-INS - RED",
			"scope": "skibidipp.builtins",
			"settings": {
				"foreground": "#f7768e",
				"fontStyle": "bold"
			}
		}
	]
}
