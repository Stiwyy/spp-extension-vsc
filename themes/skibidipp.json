{
	"$schema": "vscode://schemas/color-theme",
	"name": "Skibidi++ Tokyo Night",
	"type": "dark",
	"colors": {
		"editor.background": "#1a1b26",
		"editor.foreground": "#a9b1d6",
		"editorLineNumber.foreground": "#565f89",
		"editorLineNumber.activeForeground": "#7aa2f7",
		"editor.selectionBackground": "#28344e",
		"editor.findMatchBackground": "#3d59a1",
		"editor.findMatchHighlightBackground": "#3d59a159",
		"editorCursor.foreground": "#c0caf5",
		"editor.lineHighlightBackground": "#292e42",
		"editorIndentGuide.background": "#292e42",
		"editorIndentGuide.activeBackground": "#3b4261"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": ["comment", "comment.line", "comment.block"],
			"settings": {
				"foreground": "#565f89"
			}
		},
		{
			"name": "Keywords",
			"scope": ["keyword.control.skibidipp"],
			"settings": {
				"foreground": "#bb9af7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Declaration Keywords",
			"scope": ["storage.type.skibidipp"],
			"settings": {
				"foreground": "#bb9af7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": ["string.quoted.double.skibidipp"],
			"settings": {
				"foreground": "#9ece6a"
			}
		},
		{
			"name": "Numbers",
			"scope": ["constant.numeric.skibidipp"],
			"settings": {
				"foreground": "#ff9e64"
			}
		},
		{
			"name": "Booleans and Null",
			"scope": [
				"constant.language.boolean.skibidipp",
				"constant.language.null.skibidipp"
			],
			"settings": {
				"foreground": "#ff9e64"
			}
		},
		{
			"name": "Built-in Functions",
			"scope": ["support.function.skibidipp"],
			"settings": {
				"foreground": "#f7768e",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Functions",
			"scope": ["entity.name.function.skibidipp"],
			"settings": {
				"foreground": "#7aa2f7"
			}
		},
		{
			"name": "Objects",
			"scope": ["variable.other.object.skibidipp"],
			"settings": {
				"foreground": "#2ac3de",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable.other.skibidipp"],
			"settings": {
				"foreground": "#c0caf5"
			}
		},
		{
			"name": "Operators",
			"scope": ["keyword.operator.skibidipp"],
			"settings": {
				"foreground": "#89ddff"
			}
		},
		{
			"name": "Escape Characters",
			"scope": ["constant.character.escape.skibidipp"],
			"settings": {
				"foreground": "#f7768e"
			}
		}
	]
}
